{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "https://www.bilibili.com/movie/*"],
      "js": ["scripts/content.js"]
    }
  ]
}
